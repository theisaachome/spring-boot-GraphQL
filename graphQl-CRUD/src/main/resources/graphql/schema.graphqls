type Coffee {
  id: ID!
  name: String
  size: Size
}
enum Size {
  SHORT
  TALL
  GRANDE
  VENTI
}

type Query {
  allCoffee: [Coffee]!
  getCoffee(id: ID!): Coffee
}

type Mutation {
  createCoffee(name: String, size: Size): Coffee
  deleteCoffee(id: ID!): Coffee
  updateCoffee(id: ID!, name: String, size: Size): Coffee
}
